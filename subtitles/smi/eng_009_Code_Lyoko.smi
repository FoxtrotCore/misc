<SYNC Start="655040"><P Class="ENUSCC"></P></SYNC>
<SYNC Start="655690"><P Class="ENUSCC">Transfer Odd.</P></SYNC>
<SYNC Start="657410"><P Class="ENUSCC"></P></SYNC>
<SYNC Start="657940"><P Class="ENUSCC">Scanner Odd.</P></SYNC>
<SYNC Start="659350"><P Class="ENUSCC"></P></SYNC>
<SYNC Start="659730"><P Class="ENUSCC">Scanner Ulrich.</P></SYNC>
<SYNC Start="661460"><P Class="ENUSCC"></P></SYNC>
